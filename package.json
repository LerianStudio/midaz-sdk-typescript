{
  "name": "midaz-sdk",
  "version": "0.1.0",
  "description": "Official TypeScript SDK for the Midaz financial API - Build powerful financial applications with type-safe, modern JavaScript/TypeScript",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "npm run clean && npm run build:cjs && npm run build:esm && npm run build:types",
    "build:cjs": "tsc --project tsconfig.json",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build:types": "tsc --project tsconfig.types.json",
    "build:watch": "npm run build:cjs -- --watch",
    "clean": "rimraf dist coverage .tsbuildinfo",
    "dev": "npm run build:watch",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "test:coverage": "jest --coverage --passWithNoTests",
    "test:ci": "jest --coverage --ci --runInBand --passWithNoTests",
    "lint": "eslint src tests examples",
    "lint:fix": "eslint src tests examples --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\" \"examples/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\" \"examples/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "prepare": "npm run build",
    "postinstall": "husky install",
    "prepublishOnly": "npm run clean && npm run build && npm run test && npm run lint",
    "docs": "typedoc --options typedoc.json",
    "docs:serve": "npm run docs && npx http-server docs -p 8080 -o",
    "size": "npm run build && bundlewatch",
    "examples": "npm run example:workflow",
    "example:workflow": "npx ts-node examples/workflow.ts",
    "example:client-config": "npx ts-node examples/client-config-example.ts",
    "example:api-versioning": "npx ts-node examples/api-versioning-example.ts",
    "example:cache": "npx ts-node examples/cache-example.ts",
    "example:concurrency": "npx ts-node examples/concurrency-example.ts",
    "example:validation": "npx ts-node examples/validation-example.ts",
    "example:error-handling": "npx ts-node examples/error-handling-example.ts",
    "example:network": "npx ts-node examples/network-example.ts",
    "example:data": "npx ts-node examples/data-example.ts",
    "example:observability": "npx ts-node examples/observability-example.ts"
  },
  "keywords": [
    "midaz",
    "financial",
    "ledger",
    "api",
    "sdk",
    "typescript",
    "javascript",
    "client",
    "library",
    "fintech",
    "accounting",
    "transactions",
    "balance",
    "payments"
  ],
  "files": [
    "dist/",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/LerianStudio/midaz-sdk-typescript.git"
  },
  "homepage": "https://github.com/LerianStudio/midaz-sdk-typescript#readme",
  "bugs": {
    "url": "https://github.com/LerianStudio/midaz-sdk-typescript/issues"
  },
  "author": {
    "name": "Lerian Studio",
    "email": "developers@lerianstudio.com",
    "url": "https://lerianstudio.com"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=18.18.0 <25",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "pino": "^9.6.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.0",
    "@commitlint/config-conventional": "^19.7.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.27.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.3.5",
    "@semantic-release/npm": "^12.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.21",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.32.1",
    "bundlewatch": "^0.3.3",
    "dotenv": "^16.5.0",
    "eslint": "^9.27.0",
    "eslint-import-resolver-typescript": "^4.3.5",
    "eslint-plugin-import": "^2.31.0",
    "husky": "^9.1.7",
    "jest": "^29.5.0",
    "lint-staged": "^15.3.0",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "semantic-release": "^23.0.8",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "typedoc": "^0.28.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1"
  },
  "peerDependencies": {
    "@types/pino": "^7.0.4"
  },
  "peerDependenciesMeta": {
    "@types/pino": {
      "optional": true
    }
  },
  "bundlewatch": {
    "files": [
      {
        "path": "./dist/index.js",
        "maxSize": "100kb"
      },
      {
        "path": "./dist/esm/index.js",
        "maxSize": "100kb"
      }
    ]
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
